#include<stdio.h>
int main()
{int i,j,arr[3][3];
 printf("Enter element of matrix");
 for(i=0;i<3;i++)
 {for(j=0;j<3;j++)
  {scanf("%d",&arr[i][j]);
  }
 }
for(i=0;i<3;i++)
{for(j=0;j<3;j++)
{printf("%d \t ",arr[i][j]);
}
printf("\n");
}
for(i=0;i<3;i++)
{for(j=0;j<3;j++)
 {if(i+j==2)
  printf(" transverse diagnal element is %d ",arr[i][j]);
 }
 printf("\n");
}
return 0;
}
