#include<stdio.h>
int main()
{int number,rem1,rem2;
 printf("To check whether a number is divisible by 5 and 11.\nEnter a number ");
 scanf("%d",&number);
 rem1=(number%5);
 rem2=(number%11);
 if(rem1!=0&&rem2!=0)
  {printf("The number is not divisible by 5 and 11.");
  }
 else if(rem1==0&&rem2==0)
  {printf("The number is divisble by 5 and 11.");
  }
 else if(rem1==0&&rem2!=0)
  {printf("The number is divisible by 5 and not 11.");
  }
 else if(rem1!=0&&rem2==0)
  {printf("The number is not divisible by 5 but divisible by 11.");
  }
 else
  
 return 0;
}
